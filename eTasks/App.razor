@inherits AppBase

<Router AppAssembly="@typeof(eTasks.View.Pages.Home).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" />
        <FocusOnNavigate RouteData="@routeData" Selector="h1" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(eTasks.Components.Layouts.EmptyLayout)">
            <PageNotFound />
        </LayoutView>
    </NotFound>
</Router>
