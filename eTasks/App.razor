<Router AppAssembly="@typeof(eTasks.View.Layout.MainLayout).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(eTasks.View.Layout.MainLayout)" />
        <FocusOnNavigate RouteData="@routeData" Selector="h1" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(eTasks.View.Layout.MainLayout)">
            <PageNotFound />
        </LayoutView>
    </NotFound>
</Router>
